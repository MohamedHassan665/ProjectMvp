        android:supportsRtl="true"
        android:theme="@style/Theme.Project">
        <activity android:name=".ui.Sport"></activity>
        <activity android:name=".ui.Time"></activity>
        <activity android:name=".ui.Love"></activity>
        <activity android:name=".ui.Life" />
