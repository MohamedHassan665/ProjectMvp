            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MoreInformation" />
        <activity android:name=".ui.Faviourt" />
    </application>

</manifest>